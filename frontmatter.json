{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Título",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Descripción",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Fecha de publicación",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Previsualización",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categorías",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        },
        {
          "title": "url",
          "name": "url",
          "type": "string"
        },
        {
          "title": "featured_image",
          "name": "featured_image",
          "type": "string"
        },
        {
          "title": "site-sidebar-layout",
          "name": "site-sidebar-layout",
          "type": "choice",
          "choices": [
            "default"
          ]
        },
        {
          "title": "site-content-layout",
          "name": "site-content-layout",
          "type": "choice",
          "choices": [
            "default"
          ]
        },
        {
          "title": "theme-transparent-header-meta",
          "name": "theme-transparent-header-meta",
          "type": "choice",
          "choices": [
            "default"
          ]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    },
    {
      "title": "politicas-de-privacidad",
      "path": "[[workspace]]/content/politicas-de-privacidad"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "Alemania",
    "Argentina",
    "Brasil",
